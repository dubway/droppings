{
  "name": "Drop-pings",
  "version": "2.0",
  "description":
    "Unconventional inspirational drops to brighten your day",
    "icons": { "128": "icon.png" },
  "permissions": [
    "notifications",
    "http://onemorepixel.cc/*",
    "https://droppings.herokuapp.com/*",
    "http://yui.yahooapis.com/pure/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
    },
  "manifest_version": 2,

  "web_accessible_resources": [
    "48.png",
    "itp.png",
    "life.png",
    "people.png",
    "sex.png",
    "tech.png",
    "things.png",
    "work.png",
    "128.png"
  ]
}